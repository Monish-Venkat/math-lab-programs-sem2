   ],
   "source": [
    "#2nd order differential equation\n",
    "#Program 1\n",
    "from sympy import *\n",
    "x=symbols('x')\n",
    "y=Function('y')(x)\n",
    "DE=Eq(Derivative(y,x,2)-(5*Derivative(y,x))+(6*y),0)\n",
    "display(dsolve(DE))"
   ]
